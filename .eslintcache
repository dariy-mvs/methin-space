r');\n  }\n\n  // Валидация каждого поля формы. принимает поле, строку хедер или футер и на всякий случай ответ ошибки\n  formValidate(input) {\n    this.formRemoveError(input);\n    switch (input.name) {\n      case 'email':\n        const checkValidEmail = /^([\\w.%+-]+)@([\\w-]+\\.)+([\\w]{2,})$/i.test(this.state.userEmail);\n        if (!checkValidEmail) {\n          this.updateState('errorMessage', 'проверьте корректность данных');\n          this.formAddError(input);\n          return 1;\n        } return 0;\n\n        break;\n\n      case 'number':\n        const checkValidNumber = /^\\d[\\d\\(\\)\\ -]{4,14}\\d$/g.test(this.state.userNumber);\n        if (!checkValidNumber) {\n          this.updateState('errorMessage', 'проверьте корректность данных');\n          this.formAddError(input);\n          return 1;\n        } return 0;\n\n        break;\n\n      case 'message':\n        if (this.state.userMessage === '') {\n          this.formAddError(input);\n          this.updateState('errorMessage', 'проверьте корректность данных');\n          return 1;\n        } return 0;\n\n        break;\n\n      case 'name':\n        if (this.state.userName === '') {\n          this.updateState('errorMessage', 'проверьте корректность данных');\n          this.formAddError(input);\n          return 1;\n        } return 0;\n        break;\n\n      case 'accept':\n        if (this.state.userAgree !== true) {\n          this.updateState('errorMessage', 'проверьте корректность данных');\n          this.formAddError(input);\n          return 1;\n        } return 0;\n        break;\n      default: return 0;\n    }\n  }\n}\n\n// https://www.youtube.com/watch?v=-yZfdyPMzCE\n","/Users/merlin/Desktop/methin-space/src/components/HeaderBox_header.js",[],{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","severity":1,"message":"104","line":1,"column":17,"nodeType":"105","messageId":"106","endLine":1,"endColumn":26},{"ruleId":"107","severity":1,"message